{
  "name": "@mjyc/cycle-time-travel",
  "version": "0.1.0",
  "description": "A time travelling debugger for Cycle.js",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "babel src -d lib",
    "browserify": "mkdirp dist && browserify src/index.js --standalone default -t babelify --outfile dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjyc/cycle-time-travel.git"
  },
  "keywords": [
    "cycle.js",
    "debugger",
    "time",
    "travel"
  ],
  "author": "Nick Johnstone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjyc/cycle-time-travel/issues"
  },
  "homepage": "https://github.com/mjyc/cycle-time-travel",
  "dependencies": {
    "@cycle/dom": "22.3.0",
    "@cycle/http": "15.1.0",
    "@cycle/time": "0.19.0",
    "xstream": "11.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "mkdirp": "~0.5.0",
    "rimraf": "^2.6.3"
  }
}
